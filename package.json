		"url": "https://github.com/fullstackcaveman/node-api4-project/issues"
	},
	"homepage": "https://github.com/fullstackcaveman/node-api4-project#readme",
	"devDependencies": {
		"concurrently": "^6.0.1",
		"nodemon": "^2.0.7"
	},
	"dependencies": {
		"cors": "^2.8.5",
		"dotenv": "^8.2.0",
		"express": "^4.17.1",
		"helmet": "^4.4.1",
		"morgan": "^1.10.0",
		"node": "^15.12.0"
	}
}
