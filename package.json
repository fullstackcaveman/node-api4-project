{
	"name": "node-api4-project",
	"version": "1.0.0",
	"description": "In this challenge, you will **deploy** an API of your choosing to Heroku.",
	"main": "server.js",
	"type": "module",
	"scripts": {
		"start": "node server/server.js",
		"server": "nodemon server/server.js",
		"client": "npm start --prefix client",
		"dev": "concurrently \"npm run server\" \"npm run client\"",
		"heroku-postbuild": "cd client && npm i && npm run build"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/fullstackcaveman/node-api4-project.git"
	},
